@model List<Student>
@{
    ViewBag.Title = "Student";
}


<section class="container-fluid">
    <table class="table table-hover">
        <thead>
        <th>Id</th>
        <th>Roll</th>
        <th>Last Name</th>
        <th>Middle Name</th>
        <th>Last Name</th>
        </thead>
        @foreach (var student in @Model)
        {
            <tr>
                <td>@student.StudentId</td>
                <td>@student.Roll</td>
                <td>@student.LastName</td>
                <td>@student.MiddleName</td>
                <td>@student.FirstName</td>
            </tr>
        }
    </table>
</section>